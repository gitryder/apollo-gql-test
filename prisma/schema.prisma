generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native"]
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
}

model Drink {
  @@map(name: "drinks")
  id String @id 
  name String
  description String
  prepTime Int  
  hasAlcohol Boolean @default(true)
  image String
}